// Maximal expected amount of gold that can be collected in B steps"
"expgold": R{"rew_gold"}max=? [C<=B];

// Minimal expected number of steps to collect at least GOLD_TO_COLLECT gold and at least GEM_TO_COLLECT gem
"expsteps": Tmin=? [F "success"];

// Maximal probability that >=GOLD_TO_COLLECT gold and >=GEM_TO_COLLECT gem is collected within <=B steps
"prgoldgem": Pmax=? [F<=B "success"];

