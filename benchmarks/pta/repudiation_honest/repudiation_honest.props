const int T;

// Minimum probability that the protocol terminates successfully before time T
// RESULT (T=40): 0.612580
// RESULT (T=80): 0.864915
// RESULT (T=100): 0.920234
"deadline": Pmin=? [ F<T "terminated_successfully" ];

// Minimum probability that the protocol eventually terminates successfully
// RESULT: 1.0
"eventually": Pmin=? [ F "terminated_successfully" ];
