const double T;

// Expected amount of activated MAPK at time T
"activated_T": R{"activated"}=? [ I=T ];

// Expected time until all MAPK are activated at the same time instant
"activated_time": R{"time"}=? [ F kpp=N ];

//the expected number of reactions between MAPK and MAKK by time T
"reactions": R{"reactions"}=? [ C<=T ];
